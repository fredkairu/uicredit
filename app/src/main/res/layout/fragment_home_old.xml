                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/divider_margin"
                                    android:text="@string/total_saving"
                                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium" />
                            </LinearLayout>

                            <View
